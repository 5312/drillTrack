{
  "$schema": "https://schema.tauri.app/config/2",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1421"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "drilltrack",
  "mainBinaryName": "drilltrack",
  "version": "0.1.0",
  "identifier": "com.drilltrack.app",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "钻孔轨迹仪数据处理系统",
        "width": 1300,
        "height": 900,
        "useHttpsScheme": true
      }
    ]
  }
}
